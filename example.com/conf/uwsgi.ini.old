[uwsgi]
# variables
projectname = mirae
projectdomain = example.com
base = /var/www/example.com
# config
protocol = uwsgi
venv = %(base)/venv
pythonpath = %(base)/src/%(projectname)
module = %(projectname).wsgi
socket = /tmp/%(projectdomain).sock
chmod-socket = 666
logto = %(base)/logs/uwsgi.log 
process = 5
master = true
vacuum = true
